<% paypal_form_tag do %>
  <%= paypal_setup id, ammount, PaypalConfig[:business], 
    :notify_url => url_for(:host => PaypalConfig[:host], :controller => 'bookings', :action => 'paypal_ipn'), 
    :return => url_for(:host => PaypalConfig[:host], :controller => 'bookings', :action => 'paypal_success', :id => @booking.token),
    :cancel_return => url_for(:host => PaypalConfig[:host], :controller => 'bookings', :action => 'paypal_cancel', :id => @booking.token),
    :item_name => item_name,
    :currency_code => 'SEK',
    :business_key => File::read(PaypalConfig[:key]), 
    :business_cert => File::read(PaypalConfig[:cert]), 
    :business_certid => PaypalConfig[:certid] %>  
    <%#=  Fake cc number: 4746191858504663 %>
  <%= image_submit_tag 'paypal_button.gif'%>
<% end %>

<% @payments.each do |payment| %>
  <tr>
    <td><%= payment.paid? ? 'X' : '' %></td>
    <td><%=h payment.item.id %></td>
    <td><%=h payment.name %></td>
    <td><%=h payment.email %></td>
    <td><%=h payment.note %></td>
    <td><%=h payment.value %></td>
    <td><%= link_to 'Booking', polymorphic_path(payment) %></td>
    <td><%= link_to 'Show', admin_payment_path(payment) %></td>
    <td><%= link_to 'Edit', edit_admin_payment_path(payment) %></td>
    <td><%= link_to 'Destroy', admin_payment_path(payment), :method => :delete %></td>
  </tr>
<% end %>

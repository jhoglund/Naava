<h1>Klasser</h1>

<table>

<% @sessions.each do |session| %>
  <tr>
    <td>
    <%- total = session.bookings.count + session.course.bookings.count  -%>
    <b style="color:red"><%= total %></b>
    <strong>/ <%= link_to session.attendants.attending.size, admin_session_attendants_path(session) %></strong>
    </td>
    <td><%=l session.starts_at, :format => :short %></td>
    <td><%= link_to 'Show', admin_session_path(session) %></td>
  </tr>
<% end %>
</table>
<%= will_paginate @sessions %>
<%= page_entries_info @sessions %>

<br />

<%= link_to 'Ny klass', new_admin_session_path %>


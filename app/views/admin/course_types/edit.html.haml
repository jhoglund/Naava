%h1 Editing course
- form_for(@course, :url => admin_course_type_path(@course)) do |f|
  = render :partial => 'form', :object => f
  %p
    = f.submit 'Update'
= link_to 'Show', admin_course_type_path(@course)
|
= link_to 'Clone', clone_admin_course_type_path(@course)
|
= link_to 'Destroy', admin_course_type_path(@course), :confirm => 'Are you sure?', :method => :delete
|
= link_to 'Back', admin_course_types_path

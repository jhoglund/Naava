<h1>Listing kupponger</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Value</th>
    <th>Type</th>
  </tr>

<% @coupon_type.each do |coupon_type| %>
  <tr>
    <td><%= coupon_type.name %></td>
    <td><%= coupon_type.description %></td>
    <td><%= coupon_type.value %></td>
    <td><%= coupon_type.class.human_name %></td>
    <td><%= link_to 'Show', admin_coupon_type_path(coupon_type) %></td>
    <td><%= link_to 'Edit', edit_admin_coupon_type_path(coupon_type) %></td>
    <td><%= link_to 'Destroy', admin_coupon_type_path(coupon_type), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nytt presentkort', new_admin_coupon_type_path %>
<h1>Listing presentkort</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Value</th>
    <th>Valid from</th>
    <th>Valid to</th>
  </tr>

<% @gift_certificates.each do |gift_certificate| %>
  <tr>
    <td><%=h gift_certificate.name %></td>
    <td><%=h gift_certificate.description %></td>
    <td><%=h gift_certificate.value %></td>
    <td><%=h gift_certificate.valid_from %></td>
    <td><%=h gift_certificate.valid_to %></td>
    <td><%= link_to 'Show', admin_gift_certificate_path(gift_certificate) %></td>
    <td><%= link_to 'Edit', edit_admin_gift_certificate_path(gift_certificate) %></td>
    <td><%= link_to 'Destroy', admin_gift_certificate_path(gift_certificate), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nytt presentkort', new_admin_gift_certificate_path %>